{
  "name": "@spectrum/spectrum-css",
  "version": "2.0.0-beta.16",
  "scripts": {
    "clean": "run-p clean:temp clean:dist",
    "clean:dist": "rimraf dist",
    "clean:temp": "rimraf temp",
    "build": "run-s clean:dist mkdir balthazar postcss postcss-light postcss-dark",
    "mkdir": "mkdir -p dist/vars",
    "postcss": "postcss index.css -u postcss-nested postcss-import postcss-custom-properties postcss-calc > dist/spectrum.css",
    "postcss-light": "postcss spectrum-light.css -u postcss-nested postcss-import postcss-custom-properties postcss-calc > dist/spectrum-light.css",
    "postcss-dark": "postcss spectrum-dark.css -u postcss-nested postcss-import postcss-custom-properties postcss-calc > dist/spectrum-dark.css",
    "balthazar": "balthazar -t css -o node_modules/@spectrum/spectrum-origins/src -d dist/vars/ -c balthazar-config.json",
    "build-old": "run-s clean balthazar build:tasks-all docs-all clean:temp",
    "build:tasks-all": "node ./tasks/start-build.js",
    "lint": "eslint tasks/*.js",
    "docs-all": "run-s fetch-all docs:compile docs:copy-resources",
    "docs:compile": "topdoc",
    "docs:copy-resources": "cpx \"dist/css/**/*\" dist/docs/css/vendor/",
    "watch:docs": "onchange \"docs/**/*.styl\" -- npm run build",
    "watch:css": "onchange \"src/**/*.styl\" -- npm run build",
    "fetch-all": "copy-site-resources dist/docs",
    "livereload": "livereload \"dist\"/",
    "dev": "npm-run-all --parallel watch:* livereload",
    "tdv": "topdoc --version",
    "ci-all": "npm run build",
    "release": "dna-npm-bump-version && dna-npm-release"
  },
  "devDependencies": {
    "@spectrum/balthazar": "^0.6.1",
    "@spectrum/dna-npm-release": "^1.0.0",
    "@spectrum/dna-topdoc-template": "^0.2.0",
    "@spectrum/kulcon": "^0.0.3",
    "@spectrum/site-resources": "^0.3.0",
    "@spectrum/spectrum-origins": "^0.5.3",
    "any-promise": "^1.3.0",
    "autoprefixer": "^6.5.3",
    "bluebird": "^3.5.0",
    "cpx": "^1.5.0",
    "del": "^3.0.0",
    "eslint": "^3.12.0",
    "fs-extra": "^3.0.1",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "gulp-exec": "^2.1.3",
    "gulp-insert": "^0.5.0",
    "gulp-postcss": "^7.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.6.0",
    "gulp-util": "^3.0.8",
    "js-yaml": "^3.8.4",
    "livereload": "^0.6.0",
    "npm-run-all": "^3.1.2",
    "onchange": "^3.0.2",
    "postcss": "^5.2.6",
    "postcss-calc": "^6.0.0",
    "postcss-cli": "^4.1.0",
    "postcss-custom-properties": "^6.0.1",
    "postcss-nested": "^2.0.2",
    "postcss-svg": "^2.1.0",
    "require-dir": "^0.3.2",
    "rimraf": "^2.5.4",
    "run-sequence": "^1.2.2",
    "stylus": "^0.54.5",
    "svg-stylus": "0.0.5",
    "topdoc": "^1.0.5",
    "xmldoc": "^1.1.0"
  },
  "dependencies": {
    "@spectrum/spectrum-abstract-stylus": "^0.3.3",
    "@spectrum/spectrum-icons": "^0.0.2",
    "jsdom": "^11.0.0",
    "merge-stream": "^1.0.1",
    "postcss-import": "^10.0.0",
    "postcss-pxtorem": "^4.0.1"
  }
}
